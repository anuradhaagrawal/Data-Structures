import java.util.*;


public class Main {

 

    public static void main(String[] args) {

      int[] nums = {7,50,8,15,57,53};
      int prev1 = 0;
      int prev2 = 0;
      for (int i= 0; i<nums.length; i++){
        int temp = Math.max((nums[i]+prev1) ,  prev2);
          prev1 = prev2;
          prev2 = temp;
        }

        System.out.println(prev2);
        
          
        


      }
}
       
      

       
    

