class Solution {

    
    public int[] moveZeroes(int[] nums) {
        
        int indexSwap = 0;
        for (int i= 0; i< nums.length; i++) {
            if(nums[i] != 0){
                if (i != indexSwap)
                {
                nums[indexSwap] = nums[i];
                nums[i] = 0;
                }
                
                indexSwap++;
        }
        }
        return nums;
        
    }
}
